<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <ImageView
        android:id="@+id/banner_top"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:visibility="gone"/>
    <ImageView
        android:id="@+id/banner_bottom"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:layout_alignParentBottom="true"
        android:visibility="gone"/>
    <com.ps.novelapp.X5WebView
        android:id="@+id/full_web_webview"
        android:layout_height="fill_parent"
        android:layout_width="fill_parent"
        android:layout_below="@id/banner_top"
        android:layout_above="@id/banner_bottom"
        android:scrollbars="horizontal">
    </com.ps.novelapp.X5WebView>
    <com.ps.novelapp.X5WebView
        android:id="@+id/ad_web_webview"
        android:layout_height="fill_parent"
        android:layout_width="fill_parent"
        android:scrollbars="horizontal"
        android:visibility="gone">
    </com.ps.novelapp.X5WebView>
</RelativeLayout>